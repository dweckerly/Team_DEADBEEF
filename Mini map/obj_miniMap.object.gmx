    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*set new postion cordiantes
  so map will move with player
  with repsect to the game view*/
xp = view_xview[0] + 560
yp = view_yview[0] + 370


/*set the background color and draw 
  the rectangle for the mini map*/
draw_set_alpha(.5)
draw_rectangle(xp, yp, xp+width, yp+height, false)


//draw smart enenmies on map
draw_set_color(c_yellow)
draw_set_alpha(1)

var smartCount;
smartCount = instance_number(obj_smart_enemy)

for (i=0; i &lt; smartCount; i++)
{
    smartId = instance_find(obj_smart_enemy, i)
    var wherex = xp + smartId.x*(xscale)
    var wherey = yp + smartId.y*(yscale)
    draw_circle(wherex, wherey, 2,false)
}


//draw random enemnies on map
draw_set_color(c_yellow)
draw_set_alpha(1)

var randCount;
randCount = instance_number(obj_enemy_rand)

for (i=0; i &lt; randCount; i++) 
{
    randId = instance_find(obj_enemy_rand, i)
    var wherex = xp + randId.x*(xscale)
    var wherey = yp + randId.y*(yscale)
    draw_circle(wherex, wherey, 2,false)
}


//draw the player on the map
draw_set_color(c_lime)
draw_set_alpha(1)

var playerCount;
playerCount = instance_number(obj_player)

for (i=0; i &lt; playerCount; i++) 
{
  playerId = instance_find(obj_player, i)
  var wherex = xp + playerId.x*(xscale)
  var wherey = yp + playerId.y*(yscale)
  draw_circle(wherex, wherey, 2,false)
}


//draw the border of the map
draw_set_color(c_white)
draw_rectangle(xp, yp, xp+width, yp+height, true)





</string>
