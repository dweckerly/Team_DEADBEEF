    //if there is no floor to walk on choose a new direction
    if(!enemy_find_floor(obj_enemy_rand)){
        h_or_v = irandom_range(0, 1);
        dir = irandom_range(0, 1);
    }
} else {
    hspeed = 0;
    vspeed = 0;
}

if(check_for_object(obj_enemy_rand, 32)){
    room_restart();
}
</string>
