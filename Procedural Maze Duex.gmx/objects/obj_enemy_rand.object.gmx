            <kind>1</kind>
            <string>///random movement if player moving

//if player moving
if(global.moving){
    //horizontal movement
    if(h_or_v == 0){
        //positive x value
        if(dir == 0){
            hspeed = spd;
        }
        //negative x value
        else {
